  },
  "homepage": "https://github.com/rdkcentral/mock-firebolt#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "jest": "^27.0.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "kill-port": "^2.0.1"
